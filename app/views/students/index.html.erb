<%= content_for(:title)       { t ".title" }       %>
<%= content_for(:description) { t ".description" } %>
<%= content_for(:image_url) { image_url 'social/alumni_facebook_card.png' } %>


<h1>
  <%= t(".our_alumni_details_html", count: (@statistics['alumni']['total'] / 10) * 10) %>
</h1>

<div class="container stories-container">
  <div id="stories_container">
    <%= render 'more_stories', stories: @stories %>
  </div>
  <div id="paginator">
    <%= link_to_next_page @stories, 'Show more stories', remote: true, id: 'load_more_stories', class: 'more-stories' %>
  </div>
</div>


<%= render "shared/testimonials_new", locals: {number: 20} %>

