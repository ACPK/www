<h1>
  <%= t(".projects_details_html", count: @client.projects("alumni_projects").length) %>
</h1>

<div class="container projects-container">
  <div id="projects_container">
    <%= render 'more_projects', projects: @projects %>
  </div>
  <div id="paginator_projects">
    <%= link_to_next_page @projects, 'Show more projects', remote: true, param_name: :project_page,id: 'load_more_projects', class: 'more-projects' %>
  </div>
</div>